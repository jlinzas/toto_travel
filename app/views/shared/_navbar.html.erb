<div class="navbar-spatial">
  <span class="span1">
    <%= link_to spaceships_path, class: "home" do %>
      <i class="roc fas fa-rocket"></i>
    <% end %>
    <% if user_signed_in? %>
      <%= link_to user_path(current_user), class: "profil" do %>
        <i class="ast fas fa-user-astronaut"></i>
      <% end %>
    <% else %>
      <%= link_to new_user_session_path, class: "nav-link" do %>
        <i class="ast fas fa-user-astronaut"></i>
      <% end %>
    <% end %>
    <a href="" class="log-in"><i class="glo fas fa-globe"></i></a>
  </span>
</div>
